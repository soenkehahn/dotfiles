{
  "[haskell]": { "editor.defaultFormatter": "haskell.haskell" },
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[markdown]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[yaml]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "breadcrumbs.enabled": true,
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingOvertype": "never",
  "editor.autoClosingQuotes": "never",
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "line",
  "editor.fontFamily": "'Ubuntu Mono Regular', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontSize": 12,
  "editor.formatOnSave": true,
  "editor.highlightActiveIndentGuide": false,
  "editor.hover.enabled": false,
  "editor.hover.sticky": false,
  "editor.lightbulb.enabled": false,
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": false,
  "editor.occurrencesHighlight": false,
  "editor.parameterHints.enabled": false,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "boundary",
  "editor.selectionHighlight": false,
  "editor.suggest.showTypeParameters": false,
  "editor.tabSize": 2,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "eslint.enable": false,
  "eslint.quiet": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.ammonite": true,
    "**/.bloop": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.metals": true,
    "**/target/**": true
  },
  "git.ignoreMissingGitWarning": true,
  "go.alternateTools": { "go": "gotip" },
  "go.lintOnSave": "off",
  "go.lintTool": "golint",
  "gopls": { "ui.semanticTokens": true },
  "haskell.plugin.hlint.globalOn": false,
  "js/ts.implicitProjectConfig.strictNullChecks": true,
  "keyboard.dispatch": "keyCode",
  "python.formatting.blackPath": "/usr/bin/black",
  "python.formatting.provider": "black",
  "python.pythonPath": "/usr/bin/python3",
  "redhat.telemetry.enabled": false,
  "rust-analyzer.cargo.allFeatures": true,
  "rust-analyzer.cargoRunner": "foo",
  "rust-analyzer.completion.addCallArgumentSnippets": false,
  "rust-analyzer.completion.addCallParenthesis": false,
  "rust-analyzer.completion.autoimport.enable": false,
  "rust-analyzer.completion.postfix.enable": false,
  "rust-analyzer.hoverActions.debug": false,
  "rust-analyzer.hoverActions.enable": false,
  "rust-analyzer.hoverActions.gotoTypeDef": false,
  "rust-analyzer.hoverActions.implementations": false,
  "rust-analyzer.hoverActions.linksInHover": false,
  "rust-analyzer.hoverActions.run": false,
  "rust-analyzer.inlayHints.chainingHints": false,
  "rust-analyzer.inlayHints.enable": false,
  "rust-analyzer.inlayHints.parameterHints": false,
  "rust-analyzer.inlayHints.typeHints": false,
  "rust-analyzer.lens.debug": false,
  "rust-analyzer.lens.enable": false,
  "rust-analyzer.lens.implementations": false,
  "rust-analyzer.lens.run": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontSize": 10,
  "vim.leader": "h",
  "vim.normalModeKeyBindingsNonRecursive": [
    { "before": ["<leader>", "."], "commands": ["breadcrumbs.focusAndSelect"] },
    { "before": ["<leader>", ","], "commands": ["breadcrumbs.focus"] },
    { "before": ["<leader>", "k"], "commands": ["editor.action.commentLine"] },
    {
      "before": ["<leader>", "h"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["<leader>", "g"],
      "commands": ["editor.action.revealDefinition"]
    },
    { "before": ["<leader>", "n"], "commands": ["explorer.newFile"] },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["workbench.action.files.saveFiles"]
    },
    {
      "before": ["<leader>", "v"],
      "commands": ["workbench.action.navigateBack"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.previousEditor"]
    },
    { "before": ["<leader>", "o"], "commands": ["workbench.action.quickOpen"] },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.showCommands"]
    }
  ],
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    { "before": ["<leader>", "k"], "commands": ["editor.action.commentLine"] },
    {
      "before": ["<leader>", "h"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.previousEditor"]
    }
  ],
  "window.menuBarVisibility": "toggle",
  "window.zoomLevel": 4,
  "workbench.colorTheme": "Base16 Summerfruit Dark",
  "workbench.editor.enablePreview": false,
  "workbench.editorAssociations": { "*.ipynb": "jupyter.notebook.ipynb" },
  "workbench.fontAliasing": "none",
  "workbench.startupEditor": "newUntitledFile"
}
