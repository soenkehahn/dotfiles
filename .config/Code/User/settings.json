{
  "[haskell]": { "editor.defaultFormatter": "haskell.haskell" },
  "[html]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[javascript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[json]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[jsonc]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[markdown]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescript]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[typescriptreact]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "[yaml]": { "editor.defaultFormatter": "esbenp.prettier-vscode" },
  "breadcrumbs.enabled": true,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingOvertype": "never",
  "editor.autoClosingQuotes": "never",
  "editor.codeLens": false,
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "line",
  "editor.fontFamily": "'Ubuntu Mono Regular', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontSize": 12,
  "editor.formatOnSave": true,
  "editor.guides.highlightActiveIndentation": false,
  "editor.guides.indentation": false,
  "editor.hover.enabled": true,
  "editor.hover.sticky": false,
  "editor.lightbulb.enabled": "off",
  "editor.lineNumbers": "on",
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.occurrencesHighlight": "off",
  "editor.parameterHints.enabled": false,
  "editor.renderLineHighlight": "none",
  "editor.renderWhitespace": "trailing",
  "editor.selectionHighlight": false,
  "editor.showFoldingControls": "never",
  "editor.suggest.matchOnWordStartOnly": false,
  "editor.suggest.showTypeParameters": false,
  "editor.tabSize": 2,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "eslint.enable": false,
  "eslint.quiet": true,
  "files.associations": { "*.deno": "typescript" },
  "files.exclude": { "**/.direnv/": true },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.ammonite": true,
    "**/.bloop": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.metals": true,
    "**/target/**": true
  },
  "git.ignoreMissingGitWarning": true,
  "git.openRepositoryInParentFolders": "never",
  "go.alternateTools": { "go": "gotip" },
  "go.enableCodeLens": { "runtest": false },
  "go.lintOnSave": "off",
  "go.lintTool": "golint",
  "gopls": { "ui.semanticTokens": true },
  "haskell.plugin.ghcide-code-actions-fill-holes.globalOn": false,
  "haskell.plugin.ghcide-completions.config.autoExtendOn": false,
  "haskell.plugin.ghcide-completions.globalOn": false,
  "haskell.plugin.hlint.diagnosticsOn": false,
  "haskell.plugin.rename.config.crossModule": true,
  "js/ts.implicitProjectConfig.strictNullChecks": true,
  "keyboard.dispatch": "keyCode",
  "nix.enableLanguageServer": true,
  "nix.serverPath": "nil",
  "nix.serverSettings": {
    "nil": { "formatting": { "command": ["nixpkgs-fmt"] } }
  },
  "prettier.trailingComma": "all",
  "python.formatting.blackPath": "/usr/bin/black",
  "python.formatting.provider": "black",
  "python.pythonPath": "/usr/bin/python3",
  "redhat.telemetry.enabled": false,
  "rust-analyzer.diagnostics.disabled": ["type-mismatch"],
  "rust-analyzer.imports.granularity.group": "preserve",
  "rust-analyzer.inlayHints.parameterHints.enable": false,
  "rust-analyzer.inlayHints.typeHints.enable": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.fontSize": 10,
  "vim.joinspaces": false,
  "vim.leader": "h",
  "vim.normalModeKeyBindingsNonRecursive": [
    { "before": ["<leader>", "."], "commands": ["breadcrumbs.focusAndSelect"] },
    { "before": ["<leader>", ","], "commands": ["breadcrumbs.focus"] },
    { "before": ["<leader>", "k"], "commands": ["editor.action.commentLine"] },
    {
      "before": ["<leader>", "h"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["<leader>", "g"],
      "commands": ["editor.action.revealDefinition"]
    },
    { "before": ["<leader>", "n"], "commands": ["explorer.newFile"] },
    {
      "before": ["<leader>", "w"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "a"],
      "commands": ["workbench.action.files.saveFiles"]
    },
    {
      "before": ["<leader>", "v"],
      "commands": ["workbench.action.navigateBack"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.previousEditor"]
    },
    { "before": ["<leader>", "o"], "commands": ["workbench.action.quickOpen"] },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "t"],
      "commands": ["workbench.action.editor.nextChange"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    { "before": ["<leader>", "d"], "commands": ["editor.action.showHover"] }
  ],
  "vim.useSystemClipboard": true,
  "vim.visualModeKeyBindingsNonRecursive": [
    { "before": ["<leader>", "k"], "commands": ["editor.action.commentLine"] },
    {
      "before": ["<leader>", "h"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["<leader>", "p"],
      "commands": ["workbench.action.showCommands"]
    },
    {
      "before": ["<leader>", "c"],
      "commands": ["workbench.action.nextEditor"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.previousEditor"]
    },
    {
      "before": ["<leader>", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    { "before": ["<leader>", "d"], "commands": ["editor.action.showHover"] }
  ],
  "window.menuBarVisibility": "toggle",
  "workbench.colorTheme": "Base16 Dracula",
  "workbench.editor.enablePreview": false,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.excludeDirty": true,
  "workbench.editor.limit.value": 5,
  "workbench.startupEditor": "newUntitledFile"
}
